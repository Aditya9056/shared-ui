{
  "name": "@wpmudev/shared-ui",
  "version": "2.0.1",
  "description": "For internal use in WPMU DEV plugins",
  "author": "WPMU DEV",
  "license": "GPL-2.0",
  "homepage": "https://wpmudev.github.io/shared-ui/",
  "browserslist": [
    "> 1%",
    "Last 2 versions",
    "not ie <= 8"
  ],
  "eslintConfig": {
    "extends": "wordpress"
  },
  "eslintIgnore": [
    "a11y-dialog.js",
    "clipboard.js",
    "gulpfile.js"
  ],
  "files": [
    "dist/",
    "js/",
    "scss/"
  ],
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "chalk": "^2.3.0",
    "eslint-config-wordpress": "^2.0.0",
    "fs": "0.0.1-security",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-clean-css": "^3.9.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^4.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^3.0.0",
    "gulp-watch": "^4.3.11",
    "pump": "^2.0.0"
  },
  "style": "dist/css/shared-ui.css",
  "sass": "scss/shared-ui.scss",
  "main": "dist/js/shared-ui",
  "scripts": {
    "dev": "gulp dev",
    "build": "gulp build",
    "version": "npm version patch --no-git-tag-version && gulp version:update && npm run build",
    "release": "npm run version && git commit -m ':package:' && npm version patch --allow-same-version && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wpmudev/shared-ui.git"
  }
}
